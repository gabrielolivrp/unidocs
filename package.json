{
  "name": "unidocs",
  "private": true,
  "workspaces": [
    "packages/**",
    "apps/**"
  ],
  "scripts": {
    "build": "npx turbo run build",
    "test": "npx turbo run test",
    "dev": "npx turbo run dev --no-cache --parallel",
    "contracts:test": "npx turbo run contracts:test --filter=contracts",
    "contracts:deploy": "npx turbo run contracts:deploy --filter=contracts",
    "contracts:compile": "npx turbo run contracts:compile --filter=contracts",
    "contracts:chain": "npx turbo run contracts:chain --filter=contracts"
  },
  "devDependencies": {
    "turbo": "^1.13.3"
  }
}
